{
  "metadata": [
    {
      "src": [
        {
          "files": ["*/*.csproj"],
          "src": "../ServiceStack/ServiceStack.Blazor/src/",
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        },
        {
          "files": ["*/*.csproj"],
          "src": "../ServiceStack/ServiceStack.OrmLite/src/",
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/ServiceStack.OrmLite.T4**"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "resource": [
      {
        "files" : [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ]
      }
    ]
  }
}
